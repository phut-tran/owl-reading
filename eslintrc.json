{
  "extends": ["react-app", "react-app/jest", "plugin:cypress/recommended"],
  "rules": {
    "quotes": ["warn", "single"],
    "eol-last": ["warn", "always"],
    "semi": ["warn", "never"],
    "jsx-quotes": ["warn", "prefer-single"]
  },
  "plugins": ["eslint-plugin-cypress"],
  "env": {
    "cypress/globals": true
  },
  "root": true
}
